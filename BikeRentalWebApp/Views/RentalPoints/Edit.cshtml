@model BikeRentalWebApp.Models.RentalPointViewModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>RentalPointViewModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-controller="RentalPoints" asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Miasto" class="control-label"></label>
                <input asp-for="Miasto" class="form-control" />
                <span asp-validation-for="Miasto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ulica" class="control-label"></label>
                <input asp-for="Ulica" class="form-control" />
                <span asp-validation-for="Ulica" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Numer" class="control-label"></label>
                <input asp-for="Numer" class="form-control" />
                <span asp-validation-for="Numer" class="text-danger"></span>
            </div>
            <div class="form-group">
                <br />
                <input asp-action="Edit" asp-controller="RentalPoints" type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <br />
    <a class="btn btn-secondary" asp-controller="Rentalpoints" asp-action="List">Back to List</a>
